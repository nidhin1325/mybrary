<h2>Search User</h2>
<form action="/users" method="GET">
  <div>
    <label>Name</label>
    <input type="text" name="title" value="<%= searchOptions.title %>">
  </div>
  </div>
  <button type="submit">Search</button>
</form>
<div>
  <% users.forEach(user => { %>
    <a href="/users/<%= user.id %>">
    <img height="150" width="100" src="<%= user.coverImagePath %>">
    </a>
  <% }) %>
</div>